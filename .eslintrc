{
    "parser": "babel-eslint",
    "extends": [
//        "standard",
//        "standard-react",
//        "prettier/standard",
//        "prettier/react",
//        "plugin:prettier/recommended",
        "plugin:react-hooks/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "ecmaFeatures": {
            "legacyDecorators": true,
            "jsx": true
        }
    },
    "settings": {
        "react": {
            "version": "16"
        }
    },
    "rules": {
        "space-before-function-paren": 0,
        "react/prop-types": 0,
        "react/jsx-handler-names": 0,
        "react/jsx-fragments": 0,
        "react/no-unused-prop-types": 0,
        "import/export": 0
    }
}
