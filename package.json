{
  "name": "react-select-table",
  "version": "5.0.0",
  "description": "React table component with selectable items",
  "author": "@BanForFun",
  "license": "MIT",
  "repository": "@BanForFun/react-select-table",
  "keywords": [
    "react",
    "table",
    "listview",
    "list",
    "redux",
    "drag",
    "selection",
    "pagination",
    "columns",
    "resizable",
    "touch"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "typings": "dist/typings/index.d.ts",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clear-definitions": "rm -rf dist/typings/",
    "copy-definitions": "rsync -a -m --include '*/' --include '*.d.ts' --exclude '*' src/ dist/typings/",
    "build-definitions": " yarn clear-definitions && tsc && yarn copy-definitions",
    "build": "yarn build-definitions && microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.9",
    "cross-env": "^7.0.2",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^5.0.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "immer": "^9.0.13",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1"
  }
}
